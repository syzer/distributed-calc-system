{
    "generator-angular-fullstack": {
        "insertRoutes": true,
        "registerRoutesFile": "private/src/server/routes.js",
        "routesNeedle": "// Insert routes below",
        "routesBase": "/api/",
        "pluralizeRoutes": true,
        "insertSockets": true,
        "registerSocketsFile": "private/src/server/config/socketio.js",
        "socketsNeedle": "// Insert sockets below",
        "filters": {
            "js": true,
            "html": true,
            "css": true,
            "uirouter": true,
            "bootstrap": true,
            "uibootstrap": true,
            "socketio": true,
            "mongoose": true,
            "auth": true,
            "oauth": true,
            "googleAuth": true,
            "facebookAuth": true,
            "twitterAuth": true
        }
    },
    "generator-ng-component": {
        "routeDirectory": "private/src/client/app/",
        "directiveDirectory": "private/src/client/app/",
        "filterDirectory": "private/src/client/app/",
        "serviceDirectory": "private/src/client/app/",
        "basePath": "private/src/client",
        "filters": [
            "uirouter"
        ],
        "extensions": [
            "js",
            "html",
            "css"
        ]
    }
}
